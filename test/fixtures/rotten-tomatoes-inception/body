<script>
require([
    "jquery",
    "globals",
    "underscore",
    "backbone",
    "js-cookie",
    "bootstrap"
], function($, RT, _, Backbone, Cookies) {
    (function() {
        var pageViewCount = "pvc";
        var pageViewCountCookieValue = Cookies.get(pageViewCount);
        var date = new Date();
        var minutes = 60;
        date.setTime(date.getTime() + (minutes * 60 * 1000));
        if (pageViewCountCookieValue != null) {
            Cookies.set(pageViewCount, parseInt(pageViewCountCookieValue) + 1, {path: '/', expires: date});
        }
    }());

    (function() {
        RT.loginInit.always(function() {
            require([
                "https://staticv2.rottentomatoes.com/static/js/app/views/LoginView.js"
            ], function(LoginView) {
                RT.loginView = new LoginView.LoginModal({model: RT.loginModel, LoginModalTemplate: _.template($("script.LoginModalTemplate").html())});
                RT.loginView.render();
                RT.loginView.$el.attr("id", "login");
                RT.loginView.$el.attr("role", "dialog");
                $("body").append(RT.loginView.$el);

                RT.signupView = new LoginView.SignupModal({model: RT.loginModel, SignupModalTemplate: _.template($("script.SignupModalTemplate").html())});
                RT.signupView.render();
                RT.signupView.$el.attr("id", "signup");
                RT.signupView.$el.attr("role", "dialog");
                $("body").append(RT.signupView.$el);

                RT.passwordView = new LoginView.PasswordModal({ PasswordModalTemplate: _.template($("script.PasswordModalTemplate").html()) });
                RT.passwordView.render();
                RT.passwordView.$el.attr("id", "forgot-password");
                RT.passwordView.$el.attr("role", "dialog");
                $("body").append(RT.passwordView.$el);

                RT.desktopHeaderLogin = new LoginView.DesktopLoginArea({model:RT.loginModel, PageHeaderLoginTemplate:_.template($("script.PageHeaderLoginTemplate").html())});
                RT.desktopHeaderLogin.render();
                $("#headerUserSection").html(RT.desktopHeaderLogin.$el);

                RT.mobileHeaderLogin = new LoginView.MobileLoginArea({model:RT.loginModel, PageHeaderLoginMobileTemplate:_.template($("script.PageHeaderLoginMobileTemplate").html())});
                RT.mobileHeaderLogin.render();
                $("#navMenu .loginArea").html(RT.mobileHeaderLogin.$el);
                RT.loginModel.reloadSignIn();
            })
        });
    }());

    (function() {
        //Activate all bootstrap tooltips
        $("[data-toggle='tooltip']").tooltip({
            template: '<div class="tooltip" role="tooltip"><div class="tooltip-arrow"></div><div class="tooltip-inner"></div></div>'
        });
    }());

    (function() {
        var newsMenuLoaded = false,
            loadNewsView = function(PageHeaderView, collection, appendEl) {
                if (collection.size() > 0) {
                    var newsView = new PageHeaderView.NewsPanel({collection: collection, NewsItemTemplate: _.template($("script.NewsPageHeaderItem").html()) });
                    newsView.render();
                    appendEl.html(newsView.$el);
                }
            },
            onNewsMenuMouseEnter = function(newsItem, hubItem, totalRecallItem) {
                if (!newsMenuLoaded && !RT.NewsThrottle.header) {
                    require([RT.StaticHost + "/static/js/app/views/PageHeaderNewsView.js"], function(PageHeaderView) {
                        newsItem.fetch({
                            success: function () {
                                loadNewsView(PageHeaderView, newsItem, $("#header-news-rtnews .newsContainer"));
                            }
                        });
                        hubItem.fetch({
                            success: function () {
                                loadNewsView(PageHeaderView, hubItem, $("#header-news-guides .newsContainer"));
                            }
                        });
                        totalRecallItem.fetch({
                            success: function () {
                                loadNewsView(PageHeaderView, totalRecallItem, $("#header-news-best-worst .newsContainer"));
                            }
                        });
                    });
                }
                newsMenuLoaded = true;
            };
        require([
            RT.StaticHost + "/static/js/app/models/NewsModel.js"
        ], function(NewsModel) {
            var newsItem = new NewsModel.NewsItems({ limit: 2, excludeColumn: 39 });
            var hubItem = new NewsModel.NewsItems({ limit: 2, hub: true });
            var totalRecallItem = new NewsModel.NewsItems({ limit: 2, column: 39 });
            $("#newsMenu").mouseenter(onNewsMenuMouseEnter.bind(this, newsItem, hubItem, totalRecallItem));
        });
    }());

    (function() {
        var pageHeaderLoaded = false,
            fillTvList = function(PageHeaderView, mediaList, listContainerEl) {
                $(".title", listContainerEl).html(mediaList.title);
                var tvList = new PageHeaderView.TvList({
                    collection: new Backbone.Collection(mediaList.shows),
                    ListItemTemplate: _.template($("script.PageHeaderListItem").html())
                });
                tvList.render();
                $("table", listContainerEl).html(tvList.$el);
            },
            fillCFPList = function(PageHeaderView, promoItems, containerEl) {
                var cfpList = new PageHeaderView.CFPList({
                    collection: new Backbone.Collection(promoItems),
                    CFPItemTemplate: _.template($("script.PageHeaderCFPItem").html())
                });
                cfpList.render();
                containerEl.html(cfpList.$el);
            }
            onMovieTvMenuMouseEnter = function (pageHeaderItems, PageHeaderView) {
                if (!pageHeaderLoaded) {
                    pageHeaderItems.fetch({
                        success: function () {
                            fillCFPList(PageHeaderView, pageHeaderItems.get("movies").promoItems, $("#header-certified-fresh-picks"));
                            fillCFPList(PageHeaderView, pageHeaderItems.get("tv").promoItems, $("#header-tv-certified-fresh"));
                            fillTvList(PageHeaderView, pageHeaderItems.get("tv").mediaLists[0], $("#header-tv-col1"));
                            fillTvList(PageHeaderView, pageHeaderItems.get("tv").mediaLists[1], $("#header-tv-col2"));
                        }
                    });
                }
                pageHeaderLoaded = true;
            };

        // TODO: Figure out country settings
        require([
            RT.StaticHost + "/static/js/app/models/PageHeaderModel.js",
            RT.StaticHost + "/static/js/app/views/PageHeaderNewsView.js"
        ], function (PageHeaderModel, PageHeaderNewsView) {
            var pageHeaderItems = new PageHeaderModel.PageHeaderModel({ country: 'us' });
            $("#movieMenu, #tvMenu").mouseenter(onMovieTvMenuMouseEnter.bind(this, pageHeaderItems, PageHeaderNewsView));
        });
    }());

    RT.adRefresh = _.throttle(function () {
        googletag.pubads().refresh();
    }, 5000);

    // TODO: User Discussion
    //require([
    //    RT.StaticHost + "/static/js/app/views/SocialTool.js",
    //    "text!" + RT.StaticHost + "/templates/SocialTools.html"
    //], function(SocialTools, SocialToolTemplate) {
    //    var SocialToolOptions = {
    //        SocialToolTemplate: SocialToolTemplate
    //    };
    //    if ('mob' == options.pageId) {
    //        SocialToolOptions.movieId  = options.movieId;
    //    } else if ('tvseries' == options.pageId || 'tvseason' == options.pageId || 'tvseries' == options.pageId) {
    //        SocialToolOptions.tvSeriesId = options.tvSeriesId/**/;
    //    }
    //    var socialTools = new SocialTools(SocialToolOptions);
    //    socialTools.render();
    //    $("body").append(socialTools.$el);
    //}.bind(this));

    /** TODO: User discussion **/
    //require([RT.StaticHost + "/static/js/app/views/PageHeader.js"], function (PageHeader) {
    //    new PageHeader({
    //        el: "#headerUserSection",
    //        model: RT.LoginStatusModel,
    //        mobileEl: "#navMenu"
    //    })
    //});
    //if (RT.LoginStatusModel.userType != "anon" && Date.now() - Date.parse(RT.LoginStatusModel.registerDate) > 1800000) {
    //    dataLayer.push({'registration': RT.LoginStatusModel.userType});
    //}
    //dataLayer.push({'usertype': RT.LoginStatusModel.userType});
});
</script>
<script src="https://www.google.com/recaptcha/api.js?onload=onRecaptchaCallback&render=explicit" async defer></script>


