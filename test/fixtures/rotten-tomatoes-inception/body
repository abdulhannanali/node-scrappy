<div id="skin_ad"> 
    <script>
        require(["gpt-setup"], function(googletag){
            googletag.cmd.push(function() {
                var mapping = googletag.sizeMapping()
                    .addSize([768, 494],[[1, 1], [1,2]])
                    .build();
                var slot=googletag.defineSlot('/55153744/rottentomatoes/movie/skin', '[1,1]', 'skin_ad')
                    .defineSizeMapping(mapping)
                    .addService(googletag.pubads());
                googletag.definedSlots['skin_ad']=slot;
            });
        });
   </script>
</div>


        
        




















