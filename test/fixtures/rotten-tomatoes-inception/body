<script>
    require([
        "jquery",
        "globals",
        "underscore",
        "https://staticv2.rottentomatoes.com/static/js/app/controllers/RatingController.js",
        "domReady!"
    ],function($, RT, _, Rating){
        RT.loginInit.always(function() {
            new Rating({
                parentEl: "#rating_widget",
